<script type="text/javascript">
//Google Feed Api
google.load("feeds", "1");

function initialize() {
var feed = new google.feeds.Feed("http://www.heiwadai-dental.com/blog/category/%E7%A9%BA%E3%81%8D%E6%99%82%E9%96%93/?feed=rss2");
feed.setNumEntries(5);
feed.load(function(result) {
if (!result.error) {
var container = document.getElementById("feed1");
for (var i = 0; i < result.feed.entries.length; i++) {
var entry = result.feed.entries[i];
var dd = new Date(entry.publishedDate); // now
var yearNum = dd.getYear();
if (yearNum < 2000) yearNum += 1900;
var m = dd.getMonth() + 1;
if (m < 10) {m = "0" + m;}
var d = dd.getDate();
if (d < 10) {d = "0" + d;}
var date = yearNum + "." + m + "." + d + " ";
container.innerHTML += "<li><a href='" + entry.link + "' target='_self'>" + entry.title + "</a></li>";
}
}
});
}
google.setOnLoadCallback(initialize);

</script>

