  position:   new google.maps.LatLng(32.9263901,130.77118),
  map: map,
  icon: new google.maps.MarkerImage(
    'common/images/icon_map.png',                     // url
    new google.maps.Size(82,94), // size
    new google.maps.Point(0,0),  // origin
    new google.maps.Point(30,70), // anchor
	new google.maps.Size(82, 94)      // アイコン画像の実際の表示サイズ
  )
