<script src="/common2014/js/heightLine.js" class="empty"></script>
<script>

;(function (window, $){

	var date = new Date();
	var h = date.getHours();
	var m = date.getMinutes();
	var time= h*100 + (m+9);

	if(!$("body").hasClass('top')){
		if(time&gt;=0400 &amp;&amp; time&lt;1000){
			$("header").css({"background-image": 'url(/common2014/img/header_bg_01.jpg)'});
		}else if(time&gt;=1000 &amp;&amp; time&lt;1500){
			$("header").css({"background-image": 'url(/common2014/img/header_bg_02.jpg)'});
		}else if(time&gt;=1500 &amp;&amp; time&lt;1900){
			$("header").css({"background-image": 'url(/common2014/img/header_bg_03.jpg)'});
		}else{
			$("header").css({"background-image": 'url(/common2014/img/header_bg_04.jpg)'});
			$("header, header a").css({"color":"#fff"});
			$(".daytime").css({"display":"none"});
			$(".night").css({"display":"block"});
		}
	}

		$("#content,header,#g_nav li").hover(
			function() {
				$m_nav.find("section").removeClass("on");
				$g_nav.find("figure").removeClass('on01 on02 on03 on04 on05 on06 on07 on08');
				$g_nav.find("figure").removeClass("figureVisible");
				}
			,
			function() {
				}
		);

		$g_nav = $("#g_nav");
		$m_nav = $("#mega_nav");
		for (n = 0; n &lt; 8; n++ ) {
			$g_nav.find("li").eq(n).hover(
				function() {
					var m = $g_nav.find("li").index(this);
					$g_nav.find("figure").removeClass('on01 on02 on03 on04 on05 on06 on07 on08').addClass("figureVisible");
					$g_nav.find("figure").addClass('on0' +String(m+1));
					$m_nav.find("section").removeClass("on");
					$m_nav.find("section").eq(m).addClass("on");
					}
				,
				function() {
					}
			);
		}
		$g_nav.hover(
			function() {
				var m = $g_nav.find("li").index(this);
				//$g_nav.find("figure").css({"display":"block"});
				}
			,
			function() {
				
				}
		);



}(this, jQuery));


;(function (window, $){
	var document = window.document;

	$(window).load(function() {
		$window = $(window);
		/* --------------------------------------------------------
		*main
		-------------------------------------------------------- */
		var isMoving = false;

		$("#pagetop dl").click(function() {
			if(isMoving == false){
				isMoving=true;
				//$("#pagetop").css({"overflow":"visible"});
				$(this).find("dd").animate({"top":-100}, 600,"easeInOutQuad").animate({"top":70},{
					duration: 600,
					easing:"easeInOutQuad",
					complete:function(){
					//$("#pagetop").css({"overflow":"hidden"});
						isMoving=false;
					},
				});
			}
		});

		$("#pagetop dl").hover(function() {
			if(isMoving ==false){
				$(this).find("dd").stop().animate({"top":40},200);
			}
		}, function() {
			if(isMoving ==false){
				$(this).find("dd").stop().animate({"top":70},200);
				}
		});

	});
}(this, jQuery));

function nav(n,l){
	$g_nav = $("#g_nav");
	$m_nav = $("#mega_nav");
		$g_nav.find("li").not(l).hover(
			function() {
				//$m_nav.find("section").eq(n).css({"display":"none"});
				}
			,
			function() {
				}
		);
		$g_nav.find(l).hover(
			function() {
				//$m_nav.find("section").eq(n).css({"display":"block"});
				}
			,
			function() {
				}
		);
		$("#content,header").hover(
			function() {
				//$m_nav.find("section").eq(n).css({"display":"block"});
				}
			,
			function() {
				}
		);

}

</script>
<script class="lastChild">
;(function (window, $){
	var document = window.document;

	$(window).load(function() {
	$window = $(window);

	setTimeout(function(){$("#wrapper").addClass('on');},100);
	setTimeout(function(){$("#main h1").addClass('on');},2400);


	var date = new Date();
	var h = date.getHours();
	var m = date.getMinutes();
	var time= h*100 + (m+9);


	if(time&gt;=0400 &amp;&amp; time&lt;1000){
		$("#main figure").css({"background-image": 'url(common2014/img/index/main_bg_01.jpg)'});
	}else if(time&gt;=1000 &amp;&amp; time&lt;1500){
		$("#main figure").css({"background-image": 'url(common2014/img/index/main_bg_02.jpg)'});
	}else if(time&gt;=1500 &amp;&amp; time&lt;1900){
		$("#main figure").css({"background-image": 'url(common2014/img/index/main_bg_03.jpg)'});
	}else{
		$("#main figure").css({"background-image": 'url(common2014/img/index/main_bg_04.jpg)'});
		$("#main, #main a").css({"color":"#fff"});
		$(".daytime").css({"display":"none"});
		$(".night").css({"display":"block"});
	}


	});
}(this, jQuery));

</script>

</div><!-- #wrapper -->





