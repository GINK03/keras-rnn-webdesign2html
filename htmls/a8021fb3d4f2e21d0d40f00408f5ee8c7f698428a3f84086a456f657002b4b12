          }
          if (e("body").hasClass("no-loaded")) e(window).on("load", function() {
            e.ua.isTouchDevice || g();
            e(".main-visual__image").addClass("is-active")
          });
          else e.ua.isTouchDevice || g(), e(".main-visual__image").addClass("is-active");
          e.ua.isTouchDevice &amp;&amp;
            (e(".invisalign__panel").find("video").remove(), e(".main-visual__canvas").remove(), e(".main-visual__image").css({
