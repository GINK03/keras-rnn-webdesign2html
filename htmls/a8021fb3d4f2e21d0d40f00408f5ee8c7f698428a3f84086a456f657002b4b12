      <script>
        (function(e, g, a) {
          var f = function() {
            this.$el = {
              concept: a(".concept"),
              doc: a(g),
              header: a(".header"),
              mainVisual: a(".main-visual"),
              popup: a(".popup"),
              smallHeader: a("#cloneHeader"),
              win: a(e),
              newsList: a(".facebook__list")
            };
            this.scrollTop = 0;
            this._init()
          };
          f.prototype._init = function() {
            this.mainVisualLayout();
            this.initGoogleMaps();
            this.initGallerySlider();
            this.hoverPanelAnimation();
            this.footerNavHover();
            this.openPopupContent();
            this.initEvents();
            this.changeMaskText();

            if ($('.contents').hasClass('contents-index')) {
              this.loadFacebookPosts();
              this.openInvisalignVideo();
            }

            var c = a("#load-contents"),
              b = a(".main-visual-mask"),
              d = a(".main-visual-text");
            a(g).bind("pjax:fetch", function() {
              a("body").css({
                "overflow-x": "hidden"
              }).addClass("used-pjax");
              d.addClass("fade-in-text");
              d.removeClass("fade-out-text");
              b.addClass("fade-out-mask");
              b.removeClass("fade-in-mask");
              c.removeClass("is-loaded")
            });
            a(g).bind("pjax:render", function() {
              setTimeout(function() {
                b.addClass("fade-in-mask");
                b.removeClass("fade-out-mask");
                d.removeClass("is-active");
                a("body").css({
                  "overflow-x": "auto"
                });
                setTimeout(function() {
                    d.addClass("fade-out-text");
                    d.removeClass("fade-in-text");
                    c.addClass("is-loaded")
                  },
                  650)
              }, 1E3)
            })
          };
          f.prototype.footerNavHover = function() {
            var c = a(".footer-nav"),
              b = a("#underline"),
              d;
            c.find("li").mouseenter(function() {
              d = a(this).index() + 1;
              a(".is-current").find(".bar").addClass("is-fade-out");
              b.addClass("is-active");
              b.removeClass("is-active--1");
              b.removeClass("is-active--2");
              b.removeClass("is-active--3");
              b.removeClass("is-active--4");
              b.removeClass("is-active--5");
              b.removeClass("is-active--6");
              b.addClass("is-active--" + d)
            });
            c.mouseleave(function() {
              b.removeClass("is-active");
              a(".is-current").find(".bar").removeClass("is-fade-out")
            })
          };
          f.prototype.changeMaskText = function() {
            var c = a(".global-nav, .footer-nav").find("li"),
              b = a(".main-visual-text");
            a(".header").find("h1").on("click", function() {
              b.html('&lt;img src="/img/common/mask_text_home.png" alt="ALIGN COUTURE DENTAL OFFICE"&gt;');
              b.css({
                top: 0
              });
              a.ua.isTouchDevice ? b.find("img").css({
                height: "auto",
                width: "100%"
              }) : b.find("img").css({
                height: "100%",
                width: "auto"
              })
            });
            c.on("click", function() {
              b.css({
                top: 188
              });
              b.find("img").css({
                height: "auto",
                width: "auto"
              });
              var c = a(this).index();
              0 === c ? (b.html('&lt;img src="/img/common/mask_text_home.png" alt="ALIGN COUTURE DENTAL OFFICE"&gt;'),
                b.css({
                  top: 0
                }), a.ua.isTouchDevice ? b.find("img").css({
                  height: "auto",
                  width: "100%"
                }) : b.find("img").css({
                  height: "100%",
                  width: "auto"
                })) : 1 === c ? b.html('&lt;img src="/img/common/mask_text_doctor.png" alt="DOCTOR"&gt;') : 2 === c ? b.html('&lt;img src="/img/common/mask_text_system.png" alt="SYSTEM"&gt;') : 3 === c ? b.html('&lt;img src="/img/common/mask_text_price.png" alt="PRICE"&gt;') : 4 === c ? b.html('&lt;img src="/img/common/mask_text_access.png" alt="ACCESS"&gt;') : 5 === c &amp;&amp; b.html('&lt;img src="/img/common/mask_text_contact.png" alt="CONTACT"&gt;')
            })
          };
          f.prototype.scrollInvisalignModel = function() {
            var c = this,
              b = a(".invisalign__model-image"),
              d;
            this.$el.win.on("scroll", function() {
              c.scrollTop = c.$el.win.scrollTop();
              d = c.scrollTop / 20;
              b.css({
                backgroundPosition: "50% " + d + "px"
              })
            })
          };
          f.prototype.mainVisualLayout = function() {
            var c = this.$el.win.height(),
              b = this.$el.header.height(),
              c = c - b;
            a(".masks").height(c)
          };
          f.prototype.initGoogleMaps = function() {
            var a = new google.maps.LatLng(35.172208, 136.911517),
              b = {
                center: a,
                disableDefaultUI: !0,
                mapTypeId: google.maps.MapTypeId.ROADMAP,
                scrollwheel: !1,
                styles: [{
                  stylers: [{
                    hue: "#000"
                  }, {
                    saturation: -200
                  }]
                }],
                zoom: 17
              },
              b = new google.maps.Map(g.getElementById("GoogleMaps"), b);
            new google.maps.Marker({
              icon: "/img/common/icon_map.png",
              map: b,
              position: a,
              title: "ALIGN COUTURE DENTAL OFFICE"
            })
          };
          f.prototype.initGallerySlider = function() {
            var c = a(".js-slider");
            c.bxSlider({
              auto: !0,
              onSlideAfter: function(a, d, e) {
                c.find("li").removeClass("is-active");
                c.find(".gallery__item--" + (e + 1)).addClass("is-active")
              },
              onSliderLoad: function() {
                c.find("li").eq(1).addClass("is-active")
              },
              pause: 6E3
            })
          };
          f.prototype.openPopupContent = function() {
            var c = this;
            a(".js-open-popup").on("click", function(b) {
              b.preventDefault();
              b = a(b.currentTarget).parents("li").index();
              var d = c.$el.doc.innerHeight(),
                e = c.$el.win.scrollTop();
              c.$el.popup.css({
                height: d
              });
              c.$el.popup.find(".popup__item").css({
                top: e + 100
              });
              c.$el.popup.fadeIn(function() {
                c.$el.popup.addClass("is-active")
              });
              c.$el.popup.find(".popup__item").eq(b - 1).fadeIn();
              a(".contents").css({
                height: d,
                left: 0,
                position: "fixed",
                top: -(e - 175)
              });
              a(".js-close-popup").on("click",
                function() {
                  c.closePopupContent(e)
                })
            })
          };
          f.prototype.closePopupContent = function(c) {
            var b = this;
            b.$el.popup.removeClass("is-active");
            setTimeout(function() {
              b.$el.popup.fadeOut(function() {
                a(".contents").css({
                  height: "auto",
                  left: "auto",
                  position: "relative",
                  top: "auto"
                });
                b.$el.popup.find(".popup__item").hide()
              })
            }, 650);
            a("html, body").animate({
              scrollTop: c
            }, 0)
          };
          f.prototype.hoverPanelAnimation = function() {
            a(".concept__panel").find(".panel__button");
            var c = a(".concept__panel").find(".panel__over"),
              b;
            a(".panel--text").hover(function() {
              b =
                a(this).index();
              0 === b ? c.eq(0).parent().addClass("is-active") : 1 === b ? c.eq(2).parent().addClass("is-active") : 2 === b &amp;&amp; c.eq(1).parent().addClass("is-active")
            }, function() {
              c.parent().removeClass("is-active")
            })
          };
          f.prototype.scrollContentAnimation = function() {
            for (var c = this, b = a(".animation-point"), d = 0, e = this.$el.win.height(), f = [], g = 0; g &lt; b.length; g++) f.push(b.eq(g).offset().top);
            this.$el.win.on("scroll", function() {
              d = c.$el.win.scrollTop();
              for (var a = 0; a &lt; f.length; a++) d &gt; f[a] - e / 2 &amp;&amp; b.eq(a).addClass("is-active")
            })
          };
          f.prototype.animatePoint = function() {
            for (var c = a(".js-animate-point"), b = [], d = 0; d &lt; c.length; d++) b.push(c.eq(d).offset().top)
          };
          f.prototype.openInvisalignVideo = function() {
            /*
             * ビデオ表示
             */
            var self = this;

            var $openVideoBtn = $('#js-open-invisalign-video');
            var $closeVideoBtn = $('.movie__close');
            var $movie = $('.movie');

            $openVideoBtn.on('click', function(e) {
              e.preventDefault();

              $movie.fadeIn(function() {
                $movie.find('.player').html('&lt;iframe width="1116" height="698" src="//www.youtube.com/embed/nCmXDtT5W38?autoplay=1&amp;rel=0" frameborder="0" allowfullscreen&gt;&lt;/iframe&gt;');
              });
            });
            $closeVideoBtn.on('click', function(e) {
              e.preventDefault();

              $movie.fadeOut(function() {
                $movie.find('.player').html('');
              });
            });
          };
          f.prototype.loadFacebookPosts = function() {
              /*
               * facebookの更新を取得
               */
              var self = this;

              $.getJSON('https://graph.facebook.com/v2.5/aligncouture/feed?access_token=561484217574102|jvyyhFoRe4bsJqi3EvUiqwuAuOg', function(json) {
                  var userData = json.data;

                  // load ID
                  var id = [];
                  for (var d = 0; d &lt; userData.length; d++) {
                      id.push(userData[d].id);
                  }

                  // load Data
                  var jqxhdr = [];
                  for (var i = 0; i &lt; id.length; i++) {
                      jqxhdr.push($.ajax({
                          url: 'https://graph.facebook.com/'+ id[i] +'?fields=full_picture,link,message,created_time,type&amp;access_token=561484217574102|jvyyhFoRe4bsJqi3EvUiqwuAuOg',
                          type: 'GET',
                          dataType: 'json'
                      }));
                  }

                  $.when.apply($, jqxhdr).done(function() {
                      var data = [];
                      var result,
                          ymd;

                      var count = 0;

                      // 表示数
                      var showNum = 2;

                      for (var i = 0; i &lt; showNum; i++) {
                          result = arguments[i];

                          if (result[0].id === "506183616200968_884907168328609") {
                            result = arguments[i + 2];
                            data.push(result[0]);
                          } else {
                            data.push(result[0]);
                          }

                          ymd = data[i].created_time.split('T')[0].split('-');

                          // メッセージが未入力の場合
                          if (data[i].message === undefined) {
                              data[i].message = 'この投稿にはメッセージがありません。クリックで投稿された写真を表示します。';
                          }

                          if (data[i].type === 'link') {
                              self.$el.newsList.append('&lt;li&gt;&lt;a href="'+ data[i].link +'" target="_blank"&gt;&lt;div class="facebook__list-thumb"&gt;&lt;span style="background-image: url('+ data[i].full_picture +');"&gt;&lt;/span&gt;&lt;/div&gt;&lt;p&gt;'+ data[i].message.substr(0,54) +'...&lt;/p&gt;&lt;/a&gt;&lt;/li&gt;');
                          } else {
                              self.$el.newsList.append('&lt;li&gt;&lt;a href="'+ data[i].link +'" target="_blank"&gt;&lt;div class="facebook__list-thumb"&gt;&lt;span style="background-image: url('+ data[i].full_picture +');"&gt;&lt;/span&gt;&lt;/div&gt;&lt;p&gt;'+ data[i].message.substr(0,54) +'...&lt;/p&gt;&lt;/a&gt;&lt;/li&gt;');
                          }
                      }

                  });
              });
          };
          f.prototype.initEvents = function() {
            this.$el.win.on("resize", a.proxy(this.mainVisualLayout, this))
          };
          a(function() {
            new f
          })
        })(this, this.document, jQuery);
        (function(e) {
          function g() {
            function a(a, b) {
              var d = document.getElementById(a).getContext("2d"),
                e = new Image;
              e.onload = function() {
                d.drawImage(e, 0, 0);
                f(a)
              };
              e.src = b
            }

            function f(a) {
              var b = e("#" + a);
              a = e(window).width();
              var d = e(".main-visual").height();
              a = Math.floor(Math.random() * a - a / 2);
              var d = Math.floor(Math.random() * d - d / 2),
                f = Math.floor(5 * Math.random());
              b.css({
                transform: "translate3d(" + a + "px, " + d + "px, 0) scale(" + f + ", " + f + ")"
              });
              setTimeout(function() {
                b.css({
                  transition: "all 1.85s cubic-bezier(0.770, 0.000, 0.275, 1.000)",
                  transform: "translate3d(0,0,0) scale(1,1)",
                  opacity: 1
                })
              }, 1E3)
            }
            a("cvs-txt-1", "img/index/canvas/cvs_txt_01.png");
